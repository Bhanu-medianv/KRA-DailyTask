{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/event-booking-system/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/event-booking-system/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/event-booking-system",
  "projects": {
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "booking": {
      "type": "application",
      "root": "apps/booking",
      "entryFile": "main",
      "sourceRoot": "apps/booking/src",
      "compilerOptions": {
        "tsConfigPath": "apps/booking/tsconfig.app.json"
      }
    },
    "event": {
      "type": "application",
      "root": "apps/event",
      "entryFile": "main",
      "sourceRoot": "apps/event/src",
      "compilerOptions": {
        "tsConfigPath": "apps/event/tsconfig.app.json"
      }
    },
    "event-booking-system": {
      "type": "application",
      "root": "apps/event-booking-system",
      "entryFile": "main",
      "sourceRoot": "apps/event-booking-system/src",
      "compilerOptions": {
        "tsConfigPath": "apps/event-booking-system/tsconfig.app.json"
      }
    },
    "notification": {
      "type": "application",
      "root": "apps/notification",
      "entryFile": "main",
      "sourceRoot": "apps/notification/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notification/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    }
  }
}